<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.alphafishprop.sonicsightreader.MainMenu">

    <ImageView android:id="@+id/image_logo"
        android:layout_height="196px"
        android:layout_width="652px"
        tools:layout_editor_absoluteY="58dp"
        tools:layout_editor_absoluteX="29dp" />

    <Button
        android:id="@+id/button_start"
        android:layout_width="338dp"
        android:layout_height="54dp"
        android:layout_marginRight="23dp"
        android:onClick="button_start_click"
        android:text="Start"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="357dp" />

    <Button android:id="@+id/button_options"
        android:layout_width="338dp"
        android:layout_height="54dp"
        android:text="Options"
        android:onClick="startSightReading"
        tools:layout_editor_absoluteY="430dp"
        android:layout_marginRight="23dp"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
